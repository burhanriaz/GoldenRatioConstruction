{
  "name": "burhan",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:pages": "ng build --configuration production",
    "postbuild:pages": "powershell -NoProfile -Command \"if (Test-Path ./docs/browser/index.html) { robocopy ./docs/browser ./docs /E | Out-Null; Remove-Item ./docs/browser -Recurse -Force }; if (Test-Path ./docs/index.html) { Copy-Item ./docs/index.html ./docs/404.html -Force }; if (!(Test-Path ./docs/.nojekyll)) { New-Item -ItemType File ./docs/.nojekyll -Force | Out-Null }\"",
    "deploy:pages": "npm run build:pages && npm run postbuild:pages",
    "preview:docs": "powershell -NoProfile -Command \"Start-Process http://localhost:5500/GoldenRatioConstruction/\" && npx http-server ./docs -p 5500",
    "clean": "powershell -NoProfile -Command \"if (Test-Path ./docs) { Remove-Item ./docs -Recurse -Force }; if (Test-Path ./dist) { Remove-Item ./dist -Recurse -Force }\"",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "font-awesome": "^4.7.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.8",
    "@angular/cli": "^19.2.8",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "angular-cli-ghpages": "^2.0.3",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2"
  }
}
